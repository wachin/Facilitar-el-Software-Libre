[Desktop Action 128x128]
Exec=which convert; if [ "$?" != "0" ];then kdialog --icon=ks-error --title="Image Resizer" --passivepopup="[Error] Please install convert command and try again."; exit 1; else COUNT="0"; COUNTFILES=$(echo %F|wc -w); COUNTFILES=$((++COUNTFILES)); DBUSREF=$(kdialog --icon=ks-resize-image --title="Image Resizer - 128x128" --progressbar "\t\t\t\t\t\t\t" $COUNTFILES); for file in %F;do COUNT=$((++COUNT)); qdbus $DBUSREF setLabelText "Resize:  ${file##*/}  [$COUNT/$((COUNTFILES-1))]"; qdbus $DBUSREF Set "" value $COUNT; cd ${file%/*}; convert -resize 128 "$file" "$(echo ${file##*/}|sed "s/.${file##/*.}//")_128p.${file##/*.}"; kdialog --icon=ks-resize-image --title="Image Resizer - 128x128" --passivepopup="[Finished]    ${file##*/}";done; qdbus $DBUSREF Set "" value $COUNTFILES; sleep 1; qdbus $DBUSREF close;fi
Icon=ks-resize-image
Name=128x128 (icon)
Name[de]=128x128 (Symbol)
Name[es]=128x128 (icono)
Name[fr]=128x128 (icône)
Name[it]=128x128 (icona)
Name[ja]=128x128 (アイコン)
Name[ko]=128x128 (아이콘)
Name[pt]=128x128 (ícone)
Name[ru]=128x128 (Икона)
Name[uk]=128x128 (Ікона)
Name[zh_CN]=128x128 (图标)
Name[x-test]=xx128x128 (icon)xx

