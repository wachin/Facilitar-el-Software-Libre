[Desktop Action RegisterNewAccount]
Exec=which megareg; if [ "$?" != "0" ];then kdialog --icon=ks-error --title="Register New Account" --passivepopup="[Error] Please install megareg command and try again."; exit 1; else /usr/share/applications/MEGA_Tools-main.sh register_new_account %F;fi
Icon=ks-mega
Name=Register New Account
Name[de]=Neuen Account Registrieren
Name[es]=Registrar Nueva Cuenta
Name[fr]=Créer un nouveau compte
Name[it]=Registra un Nuovo Account
Name[ja]=新しいアカウントを登録する
Name[ko]=새로운 계정 등록
Name[pt]=Registre Nova Conta
Name[ru]=Зарегистрировать новую учётную запись
Name[uk]=Зареєструвати новий обліковий запис
Name[zh_CN]=注册新帐户

[Desktop Action SaveUserLoginCredentials]
Exec=/usr/share/applications/MEGA_Tools-main.sh save_user_login_credentials %F
