[Desktop Entry]
Actions=RegisterNewAccount;SaveUserLoginCredentials;_SEPARATOR_;ShowCloudSpace;CreateRemoteFolder;ListFileStored;RemoveFileStored;UploadFile;SyncDirectoryTree;
Icon=ks-mega
MimeType=all/all;
ServiceTypes=KonqPopupMenu/Plugin
Type=Service
X-KDE-Priority=TopLevel
X-KDE-StartupNotify=false
X-KDE-Submenu=MEGA Tools
X-KDE-Submenu[de]=MEGA Werkzeuge
X-KDE-Submenu[es]=MEGA - Herramientas
X-KDE-Submenu[fr]=Outils MEGA
X-KDE-Submenu[it]=MEGA - Strumenti
X-KDE-Submenu[ja]=MEGA 道具
X-KDE-Submenu[ko]=MEGA 도구
X-KDE-Submenu[pt]=MEGA - Ferramentas
