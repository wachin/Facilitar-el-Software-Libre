<?xml version="1.0" encoding="utf-8"?>
 <mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="text/abk">
    <comment>Android Backup File</comment>
     <icon name="ks-android-backup-restore"/>
      <glob pattern="*.ab"/>
      <glob pattern="*.abk"/>
  </mime-type>
  <mime-type type="text/hash">
    <comment>CheckSum File</comment>
     <icon name="ks-checksum"/>
      <glob pattern="*.md5"/>
      <glob pattern="*.sha1"/>
      <glob pattern="*.sha224"/>
      <glob pattern="*.sha256"/>
      <glob pattern="*.sha384"/>
      <glob pattern="*.sha512"/>
  </mime-type>
 </mime-info>
